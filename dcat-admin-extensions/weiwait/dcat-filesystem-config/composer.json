{
    "name": "weiwait/dcat-filesystem-config",
    "alias": "",
    "description": "Laravel filesystem config for oss cos qiniu",
    "type": "library",
    "keywords": ["dcat-admin", "extension", "oss", "cos", "qiniu"],
    "homepage": "https://github.com/weiwait/dcat-filesystem-config",
    "license": "MIT",
    "authors": [
        {
            "name": "weiwait",
            "email": "mail@weiwait.cn"
        }
    ],
    "require": {
        "php": ">=8.0",
        "dcat/laravel-admin": "~2.0",
        "iidestiny/laravel-filesystem-oss": "^3.1",
        "overtrue/laravel-filesystem-qiniu": "^2.1",
        "overtrue/laravel-filesystem-cos": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Weiwait\\Filesystem\\": "src/"
        }
    },
    "extra": {
        "dcat-admin": "Weiwait\\Filesystem\\DcatFilesystemConfigServiceProvider",
        "laravel": {
            "providers": [
                "Weiwait\\Filesystem\\DcatFilesystemConfigServiceProvider"
            ]
        }
    }
}
